{
  "name": "vue-upload-wrapper",
  "version": "0.0.2",
  "description": "Transform any element into an file selector",
  "main": "dist/vue-upload-wrapper.cjs.js",
  "module": "dist/vue-upload-wrapper.es.js",
  "browser": "dist/vue-upload-wrapper.min.js",
  "dependencies": {},
  "devDependencies": {
    "bili": "^3.1.2",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^4.7.1",
    "rollup-plugin-vue": "^4.3.2",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "scripts": {
    "build": "npm run build:module && npm run build:browser",
    "build:module": "NODE_ENV=production bili --format cjs,es --plugin vue",
    "build:browser": "NODE_ENV=production bili --format umd,umd-min --module-name vue-upload-wrapper --external vue --global.vue \"Vue\" --plugin vue",
    "lint": "eslint --ext js --ext vue src",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "vue": "*",
    "vuetify": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c4software/vue-upload-wrapper.git"
  },
  "keywords": [
    "vue",
    "wrapper",
    "loader",
    "file-uploadls"
  ],
  "author": "Valentin Brosseau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c4software/vue-upload-wrapper/issues"
  },
  "homepage": "https://github.com/c4software/vue-upload-wrapper#readme"
}
